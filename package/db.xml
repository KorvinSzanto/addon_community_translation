<?xml version="1.0" encoding="UTF-8"?>
<schema
  xmlns="http://www.concrete5.org/doctrine-xml/0.5"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd"
>

  <table name="atApiToken">
    <field name="avID" type="integer" size="10">
      <unsigned/>
      <key/>
    </field>
    <field name="token" type="string" size="32">
      <notnull/>
      <fixed/>
    </field>
    <index>
      <unique/>
      <col>token</col>
    </index>
  </table>

</schema>
